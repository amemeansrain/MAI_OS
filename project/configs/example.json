      },
      {
        "id": "job2",
        "command": "./task2.sh",
        "deps": ["job1"],
        "semaphore": "sem_db"
      },
      {
        "id": "job3",
        "command": "./task3.sh",
        "deps": ["job1"],
        "semaphore": "sem_db"
      },
      {
        "id": "job4",
        "command": "./task4.sh",
        "deps": ["job2", "job3"]
      }
    ]
  }